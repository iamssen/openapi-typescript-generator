{{!
this template only use when npmName and npmVersion is inserted
--additional-properties "npmName='@org/package',npmVersion='1.0.1'"
}}
{
  "name": "{{npmName}}",
  "version": "{{npmVersion}}",
  "description": "OpenAPI client for {{npmName}}",
  "author": "OpenAPI-Generator",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@rocket-scripts/openapi": "*"
  },
  "devDependencies": {
    "typescript": "^4.0.0"
  }{{#npmRepository}},{{/npmRepository}}
  {{#npmRepository}}
  "publishConfig": {
    "registry": "{{npmRepository}}"
  }
  {{/npmRepository}}
}
