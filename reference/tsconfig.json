{
  "extends": "@rocket-scripts/web/tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@rocket-scripts/openapi": [
        "../../source/src/@rocket-scripts/openapi"
      ],
      "@rocket-scripts/openapi/*": [
        "../../source/src/@rocket-scripts/openapi/*"
      ],
      "@rocket-scripts/mockup": [
        "../../source/src/@rocket-scripts/mockup"
      ],
      "@rocket-scripts/mockup/*": [
        "../../source/src/@rocket-scripts/mockup/*"
      ]
    }
  },
  "include": [
    "src",
    "scripts"
  ]
}
